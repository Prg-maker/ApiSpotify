generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id       String     @id
  name     String
  email    String
  urlPhoto String
  type     String
  country  String
  playlist PlayList[]

}

model PlayList {
  id    String @id
  name  String
  image String


  User   User   @relation(fields: [id], references: [id])
  userId String


  musicas Musicas[]

}

model Musicas {
  id          String     @id
  name        String
  artist      String
  imageUrl    String
  preview_url String
  playlist    PlayList[]
}
